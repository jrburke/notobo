{
  "camelcase": false,
  "curly": true,
  "forin": false,
  "latedef": "nofunc",
  "newcap": false,
  "noarg": true,
  "nonew": true,
  "quotmark": "single",
  "undef": true,
  "unused": "vars",
  "strict": true,
  "trailing": true,
  "maxlen": 80,

  "eqnull": true,
  "expr": true,

  "maxerr": 1000,
  "regexdash": true,
  "laxcomma": true,
  "proto": true,

  "node": true,

  "-W078": true,

  "predef": [
    "describe",
    "it",
    "before"
  ]
}
